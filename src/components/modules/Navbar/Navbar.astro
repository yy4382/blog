---
import type { NavMenu } from "@configs/navbar";
import NavBar from "./NavBar.vue";

interface Props {
  navStats: NavMenu | string | undefined;
}
const { navStats } = Astro.props;
---

<NavBar navStats={navStats} client:load>
  <img
    src="/icon-96.png"
    alt="logo"
    class="w-8 h-8 rounded-lg"
    width={32}
    height={32}
  />
</NavBar>
